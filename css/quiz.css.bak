body{
	background-image:url("textures.jpg");
	margin:0;
	}
h1{
	font-size:25px;
	color:white;}
.navbar{
	overflow: hidden;
	background-color: white;		
	position:relative;			  
	width:auto;
	z-index: 99;
	top: 0;
	transition:0.3s;
	}
	
.navbar .logo{
	left:0;
	height:13px;
	width:100px;	
	}

.navbar img{
    left:0;
	height:75px;
	width:200px;
    }
	
.navbar .Button{
	background-color:rgb(14, 66, 161)0;
	border:none;
	opacity:1;
	color:black;
	padding: 10px 25px;
	text-align:center;
	text-decoration:none;
	display:inline-block;
	font-size: 20px;
	font-family: Arial, Helvetica, sans-serif;
	cursor: pointer;
	float: right;	
	transition: all 0.5s;
	margin:0px;
	border-radius: 12px;						
	}
	
.navbar .Button:hover{
	background-color:lightgrey;
	font-size:24px;		
	}
.icon{width:100%;
	height:180px;
	position:overflow;
	overflow:auto;
  
    margin:auto;}
.text{color:white;
	margin-top:-150px;
	font-size:80px;
	top:0;
	text-align:center;
	text-shadow: 4px 4px 70px #00FFFF;}
.tab{overflow:hidden;
	background-color:black;
	text-align:center;}
.tabelements{float:center;
	outline:black;
	font-size:20px;
	color:white;
	background-color:brown;
	cursor:pointer;
	padding:15px 63px;
	margin:1px 0px}
.tabelements:hover{
	background-color:gold;
}
.tabelements:active{
	background-color:black;
}
.quizQuestions{
	display:none;
}
#finalResults{
	display:none;
}
#results{
	border:5px solid;
	border-radius:15px;
	border-color:white;
}
#timerLabel,#showTimer{
	display:inline;
}
#timer{
	position:fixed;
	background-color:rgba(255, 0, 0, 0.6);
	width:100%;
	padding-top:10px;
	padding-bottom:10px;
	margin-top:-8px;
	margin-left:-10px;
	font-size:20px;
	text-align:center;
	font-weight:bold;
	color:white;
	font-family: Arial, sans-serif;
}
#questions{
	padding-top:50px;
	color:white;
	font-size:15px;
	font-family: Arial, sans-serif;
	margin-left:35%
}
#quizHeading,#resultsHeading{
	font-family:'Trebuchet MS', sans-serif;
	color:white;
	font-size:50px;
	text-align: center;
}
.resultsDetails{
	font-size:20px;
	font-family: "Times New Roman", Times, serif;
	color:white;
	line-height:1.5;
	margin-left:50px;
}
.quizDetails{
	text-align:center;
	line-height:0.3;
	font-size:20px;
	font-family: "Times New Roman", Times, serif;
	color:white;
}
#startButton{
	margin-top:50px;
	padding:30px 50px 30px 50px;
	
	font-weight:bold;
	
	font-size:25px;
	background-color:black;
	
	color: white;
}
#startButton:hover{
	background-color:grey;
}
.button{
	text-align:center;
}
.ques{
	font-family: "Times New Roman", Times, serif;
	font-size:18px;
	margin-top:20px;
	padding-left:50px;
	border:5px solid;
	border-radius:15px;
	border-color:white;
	width:500px;
}
.answers{
	margin-left:30px;
}
#submitButton{
	margin-top:50px;
	margin-left:200px;
	padding:10px 30px 10px 30px;
	
	font-weight:bold;
	outline-style:none;
	font-size:20px;
	background-color:black;
	
	color:#fff;
}
#submitButton:hover{
	background-color:grey;
}
#retryButton{
	margin-top:50px;
	padding:10px 30px 10px 30px;
	
	font-weight:bold;
	outline-style:none;
	font-size:20px;
	background-color:black;

	color:white;
}
#retryButton:hover{
	background-color:grey;
}
.quizAnchors{
	text-decoration:none;
	color:white;
	border:2px solid;
	border-radius:15px;
	padding-left:5px;
	padding-right:5px;
}
.quizAnchors:hover{
	background-color:white;
	color:black;
	border-color:white;
}